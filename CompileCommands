
protoc --cpp_out=. storage.proto
protoc --grpc_out=. --plugin=protoc-gen-grpc=`which grpc_cpp_plugin` storage.proto

g++ -std=c++11 `pkg-config --cflags protobuf grpc` -c -o storage.pb.o storage.pb.cc
g++ -std=c++11 `pkg-config --cflags protobuf grpc` -c -o storage.grpc.pb.o storage.grpc.pb.cc
g++ -std=c++17 `pkg-config --cflags protobuf grpc` -c -o storage.o storage_server.cpp


protoc --grpc_out=. --plugin=protoc-gen-grpc=`which grpc_cpp_plugin` service.proto
protoc --cpp_out=. service.proto